LOAD STUDENTS 
LOAD MARKS
TEMP1 <- CROSS STUDENTS, MARKS
TEMP2 <- SELECT STUDENTS_stud_id == MARKS_stud_id FROM TEMP1
TEMP3 <- SELECT maths_marks > 70 FROM TEMP2
TEMP4 <- SELECT stud_sex == 1 FROM TEMP3
TEMP5 <- SELECT advisor_id == 1 FROM TEMP4
RESULT_E <- PROJECT STUDENTS_stud_id,maths_marks FROM TEMP5
RENAME STUDENTS_stud_id TO stud_id FROM RESULT_E 
PRINT RESULT_E
CLEAR TEMP1
CLEAR TEMP2
CLEAR TEMP3
CLEAR TEMP4
CLEAR TEMP5
CLEAR STUDENTS
CLEAR MARKS