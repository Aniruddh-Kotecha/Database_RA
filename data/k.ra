LOAD EMPLOYEE
LOAD WORKS_ON
TEMP <- CROSS EMPLOYEE, WORKS_ON
TEMP1 <- SELECT Ssn == Essn FROM TEMP
TEMP2 <- SELECT Sex == 1 FROM TEMP1
TEMP3 <- SELECT Sex == 0 FROM TEMP1
TEMP4 <- CROSS TEMP2, TEMP3
TEMP5 <- SELECT TEMP2_Pno == TEMP3_Pno FROM TEMP4
RESULT_K <- PROJECT TEMP2_Pno FROM TEMP5
RENAME TEMP2_Pno TO Pno FROM RESULT_K
PRINT RESULT_K
CLEAR EMPLOYEE
CLEAR  WORKS_ON
CLEAR TEMP
CLEAR TEMP1
CLEAR TEMP2
CLEAR TEMP3
CLEAR TEMP4
CLEAR TEMP5

