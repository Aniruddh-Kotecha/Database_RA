LOAD STUDENTS 
LOAD MARKS
TEMP <- SELECT maths_marks > 90 FROM MARKS
TEMP1 <- SELECT advisor_id == 2 FROM STUDENTS
TEMP2 <- CROSS TEMP1, TEMP
TEMP3 <- SELECT TEMP1_stud_id == TEMP_stud_id FROM TEMP2
RESULT_H <- PROJECT TEMP1_stud_id, advisor_id, maths_marks FROM TEMP3
RENAME TEMP1_stud_id TO stud_id FROM RESULT_H
RENAME advisor_id TO adv_id FROM RESULT_H
PRINT RESULT_H 
CLEAR STUDENTS
CLEAR MARKS 
CLEAR TEMP
CLEAR TEMP1
CLEAR TEMP2
CLEAR TEMP3