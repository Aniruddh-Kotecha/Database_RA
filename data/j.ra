LOAD EMPLOYEE
TEMP <- CROSS EMPLOYEE, EMPLOYEE
TEMP1 <- SELECT EMPLOYEE1_Super_ssn == EMPLOYEE2_Ssn FROM TEMP
TEMP2 <- SELECT EMPLOYEE1_Bdate == EMPLOYEE2_Bdate FROM TEMP1
RESULT_J <- PROJECT EMPLOYEE1_Ssn, EMPLOYEE2_Ssn, EMPLOYEE1_Bdate FROM TEMP2
RENAME EMPLOYEE1_Ssn TO Ssn FROM RESULT_J
RENAME EMPLOYEE2_Ssn TO Super_ssn FROM RESULT_J
RENAME EMPLOYEE1_Bdate TO Bdate FROM RESULT_J
PRINT RESULT_J
CLEAR TEMP
CLEAR TEMP1
CLEAR TEMP2
CLEAR EMPLOYEE